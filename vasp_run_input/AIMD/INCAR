SYSTEM = NaK

ISTART = 0 # whether or not to read the WAVECAR file. 
ICHARG = 0 # how VASP constructs the initial charge density.
LWAVE = .FALSE. # whether the wavefunctions are written to the WAVECAR file
LCHARG = .FALSE. # LCHARG determines whether the charge densities (files CHGCAR and CHG) are written.
LVTOT = .FALSE. # whether the total local potential is written to the LOCPOT file
LVHAR = .FALSE. # whether the electrostatic potential is written to the LOCPOT file
LELF = .FALSE. # whether to create an ELFCAR file or not.
LSCALAPACK = .FALSE.
IWAVPR = 12
ADDGRID = .True.

 PREC = Normal    ! standard precision 

! MD
 ISYM = 0         ! strongly recommened for MD
 IBRION = 0       ! molecular dynamics
 MDALGO = 2
 SMASS = 1       ! Nose Hoover thermostat
 NSW = 5000       ! 5000 steps
 POTIM = 1.5      ! timestep 2 fs
 TEBEG = 2000
 TEEND = 2000 ! temperature
 ISIF = 2

! SCF
 ISMEAR = 0
 SIGMA = 0.05
 ALGO = Normal
 ENCUT = 400      ! cutoff should be set manually
 EDIFF  =   1E-06
 NELMIN = 5
 NELM = 300
 LREAL = Auto
 NCORE = 14
 IVDW = 12
 VDW_S8 = 0.7875
 VDW_A1 = 0.4289
 VDW_A2 = 4.4407
 !NELMDL = -12
